

@model Vidly.Models.Customer

@{
         ViewBag.Title = Model.Name;
         Layout = "~/Views/Shared/_Layout.cshtml";
     }

<h2>@Model.Name</h2>
<table style="width: 100%">
    <thead>
    <tr>
       <th> Name </th>
        
    
        <th>  MembershipType </th>
        
        <th> Birthday</th>
    </tr>
    </thead>
    <br />
    <tbody>
   
    
        <tr>
            <td> @Model.Name </td>
        
        
            <td> @Model.MemberShipType.Name </td>
            
            <td>
           @Model.Birthday.ToString().Substring(0,10)</td>
        </tr>
    
    </tbody>
</table>